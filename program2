#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    char ch;
    char str[100];
    char sentence[1000];
    scanf("%c", &ch);
    scanf("%*c");
    scanf("%s", str);
    scanf("%*c");
    scanf("%[^\n]%*c", sentence);
    printf("%c\n", ch);
    printf("%s\n", str);
    printf("%s\n", sentence);
    return 0;
}
